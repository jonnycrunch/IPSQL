{
  "name": "ipsql",
  "version": "0.0.0-dev",
  "description": "Decentralized SQL Database",
  "main": "index.js",
  "type": "module",
  "bin": {
    "ipsql": "./dist/esm/cli/bin.js"
  },
  "scripts": {
    "build": "npm_config_yes=true npx ipjs@latest build --tests && npm run build:bin",
    "publish": "npm_config_yes=true npx ipjs@latest publish",
    "build:bin": "chmod 755 dist/esm/cli/bin.js",
    "lint": "standard",
    "test:cjs": "npm run build && mocha dist/cjs/node-test/test-*.js",
    "test:node": "mocha test/test-*.js",
    "test": "standard && npm run test:node",
    "test:node-v12": "mocha test/test-*.js && npm run test:cjs",
    "test:node-v14": "npm run test:node-v12",
    "coverage": "c8 --reporter=html mocha test/test-*.js && npm_config_yes=true npx st -d coverage -p 8080"
  },
  "keywords": [],
  "author": "Mikeal Rogers <mikeal.rogers@gmail.com> (https://www.mikealrogers.com/)",
  "license": "(Apache-2.0 AND MIT)",
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./bin": {
      "import": "./cli/bin.js"
    },
    "./cli": {
      "import": "./cli/cli.js"
    },
    "./utils": {
      "import": "./src/utils.js"
    },
    "./cache": {
      "import": "./src/cache.js"
    },
    "./network": {
      "import": "./src/network.js"
    },
    "./csv": {
      "import": "./src/csv.js"
    },
    "./database": {
      "import": "./src/database.js"
    },
    "./json-import": {
      "import": "./src/json-import.js"
    }
  },
  "devDependencies": {
    "hundreds": "0.0.9",
    "memdown": "^5.1.0",
    "mocha": "^8.2.1",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.3.0",
    "@ipld/car": "^0.1.2",
    "@ipld/dag-cbor": "^2.0.3",
    "aws-config": "^1.4.0",
    "bent": "^7.3.12",
    "charwise": "^3.0.1",
    "chunky-trees": "^1.2.0",
    "cliffy": "^2.4.2",
    "encoding-down": "^6.3.0",
    "encrypted-block": "0.0.3",
    "get-port": "^5.1.1",
    "levelup": "^4.4.0",
    "multiformats": "^4.4.3",
    "papaparse": "^5.3.0",
    "public-ip": "^4.0.3",
    "tempy": "^1.0.0",
    "yargs": "^16.2.0",
    "znode": "^1.1.5"
  }
}
